{
    "name": "aliasay/dephotos-sdk",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.2"
    },
    "license": "MIT",
    "require": {},
    "autoload": {
        "psr-4": {
            "Depositphotos\\SDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Depositphotos\\SDK\\Tests\\": "tests"
        }
    },
    "scripts":{
        "test": "vendor/bin/phpunit tests",
        "analyse": " vendor/bin/phpstan analyse src -l 7"
    }
}
